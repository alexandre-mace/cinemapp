{% extends 'admin/base.html.twig' %}

{% block title %}Session{% endblock %}

{% block content %}
    <h1>Session</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ session.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ session.date ? session.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_session_list') }}" class="btn btn-secondary color-white">back to list</a>

    <a href="{{ path('admin_session_edit', {'id': session.id}) }}" class="btn btn-info color-white">edit</a>

    {{ include('admin/session/_delete_form.html.twig') }}
{% endblock %}
